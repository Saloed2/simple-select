 {type} = event.target.dataset;\r\n        console.log(type)\r\n        if(type == 'input'){\r\n            this.toggle()\r\n        }\r\n        if(type === 'item'){\r\n            const id = event.target.dataset.id;\r\n            const text = event.target.textContent;\r\n            console.log(id);\r\n            this.$el.querySelector('[data-type =\"input\"]').textContent = text;\r\n            this.close();\r\n        }\r\n    }\r\n    open(){\r\n        this.$el.classList.toggle('open')\r\n    };\r\n    get isOpen(){\r\n        this.$el.classList.contains('open')\r\n    }\r\n    toggle(){\r\n        this.isOpen ? this.close() : this.open();\r\n    }\r\n    close(){\r\n        this.$el.classList.remove('open')\r\n    };\r\n    destroy(){\r\n        this.$el.removeEventListenr('click', this.clickHandler);\r\n    }\r\n};"},"lineCount":null}},"error":null,"hash":"ef24e1860cfea68b061a6a250f8ce2a0","cacheData":{"env":{}}}